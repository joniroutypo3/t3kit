{
  "name": "t3kit/t3kit",
  "description": "t3kit project. A TYPO3 website starterkit.",
  "type": "typo3-cms-extension",
  "keywords": [
    "t3kit",
    "typo3",
    "cms",
    "starter",
    "t3kit10"
  ],
  "homepage": "http://t3kit.com",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "t3kit Community",
      "homepage": "https://github.com/t3kit"
    }
  ],
  "support": {
    "issues": "https://github.com/t3kit/t3kit/issues",
    "forum": "https://typo3.slack.com/messages/t3kit",
    "source": "https://github.com/t3kit/t3kit",
    "docs": "https://github.com/t3kit/t3kit"
  },
  "require": {
    "typo3/cms-backend": "dev-master",
    "typo3/cms-core": "dev-master",
    "typo3/cms-extbase": "dev-master",
    "typo3/cms-extensionmanager": "dev-master",
    "typo3/cms-filelist": "dev-master",
    "typo3/cms-filemetadata": "dev-master",
    "typo3/cms-fluid": "dev-master",
    "typo3/cms-frontend": "dev-master",
    "typo3/cms-install": "dev-master",
    "typo3/cms-recordlist": "dev-master",
    "typo3/cms-form": "dev-master",
    "typo3/cms-rte-ckeditor": "dev-master",
    "typo3/cms-seo": "dev-master",
    "typo3/cms-tstemplate": "dev-master",
    "typo3/cms-felogin": "dev-master",
    "b13/container": "dev-master"
  },
  "require-dev": {
    "editorconfig-checker/editorconfig-checker": "~10.2.3",
    "squizlabs/php_codesniffer": "~3.6.0",
    "helmich/typo3-typoscript-lint": "~2.5.0",
    "sclable/xml-lint": "~0.4.0"
  },
  "suggest": {
    "typo3/cms-indexed-search": "TYPO3 indexed search",
    "yoast-seo-for-typo3/yoast_seo": "Yoast SEO for TYPO3",
    "apache-solr-for-typo3/solr": "Apache Solr for TYPO3",
    "friendsoftypo3/frontend-editing": "Easy inline editing for TYPO3"
  },
  "autoload": {
    "psr-4": {
      "T3k\\t3kit\\": "Classes"
    }
  },
  "prefer-stable": true,
  "config": {
    "vendor-dir": "build/vendor",
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "lint-php": "phpcs --colors",
    "lint-yaml": "yaml-lint --ansi typoscript-lint.yml Resources/ Configuration/ Classes/",
    "lint-editorconfig": "ec -exclude LICENSE.txt",
    "lint-typoscript": "typoscript-lint --ansi --fail-on-warnings",
    "lint-xml": "xmllint --ansi --exclude=build/ .",
    "test": [
      "@lint-php",
      "@lint-yaml",
      "@lint-editorconfig",
      "@lint-typoscript",
      "@lint-xml"
    ]
  },
  "extra": {
    "typo3/cms": {
        "web-dir": "build/public",
        "app-dir": "build",
        "extension-key": "t3kit"
    }
  }
}
