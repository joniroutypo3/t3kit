# ###########################
# Groups of Content elements
# ===========================

# Remove default "standart" TYPO3 CE group
mod.wizards.newContentElement.wizardItems.common >

# t3kit CE groups
mod.wizards.newContentElement.wizardItems {
  static {
    header = LLL:EXT:t3kit/Resources/Private/Language/ContentElements/locallang.xlf:group.static
    before = dynamic
  }
  dynamic {
    header = LLL:EXT:t3kit/Resources/Private/Language/ContentElements/locallang.xlf:group.dynamic
    after = static
  }
}
# ___________________________

# ###########################
# Import configurations for all Content elements
# ===========================
@import 'EXT:t3kit/Configuration/TSconfig/Page/Mod/Wizards/ContentElements/Static/*.tsconfig'
@import 'EXT:t3kit/Configuration/TSconfig/Page/Mod/Wizards/ContentElements/Dynamic/*.tsconfig'
@import 'EXT:t3kit/Configuration/TSconfig/Page/Mod/Wizards/ContentElements/Special/*.tsconfig'
# ___________________________
